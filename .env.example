APP_NAME="Site Yonetimi"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_TIMEZONE=Europe/Istanbul
APP_URL=http://localhost:8000
# Production: APP_ENV=production, APP_DEBUG=false, APP_URL=https://...
APP_LOCALE=tr
APP_FALLBACK_LOCALE=en

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=siteyonetimi
DB_USERNAME=siteyonetimi
DB_PASSWORD=

BROADCAST_CONNECTION=log
CACHE_STORE=file
FILESYSTEM_DISK=local
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120
# Production: SESSION_DRIVER=database, CACHE_STORE=database, QUEUE_CONNECTION=database

MAIL_MAILER=log
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="info@siteyonetimi.test"
MAIL_FROM_NAME="${APP_NAME}"

SEED_DEMO_DATA=false

VITE_APP_NAME="${APP_NAME}"
